	@mixin rangeThumb {
	width: 13px;
	height: 13px;
	
	margin: -5px 0  0;
	border-radius: 50%;
	background: $accent3;
	cursor: pointer;
	border: 0 !important;
}

@mixin rangeTrack {
	height: 2px;
	cursor: pointer;
	background: $light;
}

.range input {
	width: 100%;
	position: relative;
	top: 2px;
	
	height: 25px; //IE
	-webkit-height: 0;
	-webkit-appearance: none;


	// Thumb
	&::-webkit-slider-thumb {
		-webkit-appearance: none; // needed again for Chrome & Safari
		@include rangeThumb;
	}

	&::-moz-range-thumb {
		@include rangeThumb;
	}

	&::-ms-thumb {
		@include rangeThumb;
	}

	// Track
	&::-webkit-slider-runnable-track {
		@include rangeTrack;
	}

	&::-moz-range-track {
		@include rangeTrack;
	}

	&::-ms-track {
		@include rangeTrack;
	}

	&:focus { // override outline/background on focus
		background: none;
		outline: none;
	}

	&::-ms-track {
		width: 100%;
		cursor: pointer;
		background: $light4;
		border-color: $light4;
		color: $light4;
	}
}

