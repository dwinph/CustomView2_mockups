.nextPrev {
  &:hover{
    width: auto;
    height: auto;
  }
}
.sign-in{
  p{
    width:85%;
    margin: 15px auto;
  }
  .input-group{
    .form-control{
        color: white;
    }
  }
}
.intro, .policy, .baro{

  h3 {
    font-size: 24px;
  }
  h1{
    margin-left: 15px;
  }

  .col-lg-7.col-md-7{
    width: 70%;
  }
}
.link-2{
  color: white;
  text-decoration: underline;
}
.h-color{
  color: #83edff;
}

.baro{
  h3{
    &.range-left, &.range-right{
      font-size: 18px;
    }
  }
  h1{
    margin-bottom: 40px;
  }
  .row.p-5{

    padding-bottom:0 !important;
  }  
}
.stmt {
  h1{
    margin-bottom: 40px;
  }
}
.range-left, .range-right{
  max-width: 300px;
  text-align: left;
}
.range-left{
}
.range-right{
  margin-left: 20px;
}

.img-range-left{
  margin-right: 30px;
}
.img-range-right{
  margin-left: 30px;
}

.img-container{
  display: flex;
  align-items: center;
  justify-content: center;
}
.img-range-left, .img-range-right, .img-range-p.p-2{
  max-width:50%;
}
.button5.submit{
  font-size: 10px;
  font-weight: 400;
}
.btnGo{
  padding-top: 3px;

}
button{
  text-align: center !important;
  padding: 0;
}
@media (min-width: 813px) {
.biographical-form{
    form{
        justify-content: flex-end;
    }
}
}
@media (max-width: 812px) and (orientation: portrait) {
.range-left, .range-right{
  text-align: left !important;
  margin: 0 !important;
}
.img-range-left{
    margin-right: 0;
  }
  .img-range-right{
    margin-left: 0;
  }
  .img-range-p.p-2 .box-wrapper{
    margin-left: -50px;
    margin-top: 30px;
  }
}
@media (max-width: 768px) and (orientation: portrait) {
.img-range-left{
    margin-right: 30px;
  }
  .img-range-right{
    margin-left: 100px;
  }
  .img-range-p.p-2 .box-wrapper{
    margin-left: 0;
    margin-top: 0;
  }
}
@media (max-width: 610px) and (orientation: portrait) {
  .sign-in{
    .input-group{
        .form-control{
            width: 300px;
        }
    }
  }
  .intro, .policy{
    .col-lg-7.col-md-7{
     width: 100%;
    }
  }
  .biographical-form{
    .form-group{
        label{
          width:100px;
        }
        .form-control{
            width: 200px;
        }
    }
  }
}

@media (max-width: 812px) and (orientation: landscape) {
  .img-range-left{
    margin-right: 20px;
  }
  .img-range-right{
    margin-left: 110px;
  }
  .range-left, .range-right{
    font-size: 16px !important;
  }

  .biographical-form{
    form{
        justify-content: flex-begin;
    }
    .form-group{
        label{
            width: 200px;
        }
        .form-control{
            width: 300px;
        }
    }
  }

}
@media (max-width: 667px) and (orientation: landscape) {
  .img-range-left{
    margin-right: 0;
  }
  .img-range-right{
    margin-left: 120px;
  }

}

@media (max-width: 375px) and (orientation: portrait) {
.img-range-left{
    margin-right: 30px;
  }
  .img-range-right{
    margin-left: 0;
  }
  .img-range-p.p-2 .box-wrapper{
    margin-left: -60px;
    margin-top: 40px;
  }
}

@import "ie_styles"
