					<!--[if lt IE 9]>
					<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
					<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
					<![endif]-->
				</head>
				<body>
					
              <%= yield %>

						<%= javascript_include_tag "https://code.jquery.com/jquery-1.12.4.js", "data-turbolinks-track" => true %><%= javascript_include_tag "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js", "data-turbolinks-track" => true %>
						<%= javascript_include_tag "https://code.jquery.com/ui/1.12.1/jquery-ui.js", "data-turbolinks-track" => true %><%= javascript_include_tag "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js", "data-turbolinks-track" => true %>
						<%= javascript_include_tag "custom/king_personal_impact_experience/assessment/survey/app.js", "data-turbolinks-track" => true %>
						
					</body>
</html>